{% extends 'base.html.twig' %}

{% block title %}Transporteurs{% endblock %}

{% block body %}
    <h1>Transporteurs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transporteur.id }}</td>
            </tr>
            <tr>
                <th>NameTransport</th>
                <td>{{ transporteur.nameTransport }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ transporteur.description }}</td>
            </tr>
            <tr>
                <th>PriceTransport</th>
                <td>{{ transporteur.priceTransport }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ transporteur.createdAt ? transporteur.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_transporteurs_index') }}">back to list</a>

    <a href="{{ path('app_transporteurs_edit', {'id': transporteur.id}) }}">edit</a>

    {{ include('transporteurs/_delete_form.html.twig') }}
{% endblock %}
